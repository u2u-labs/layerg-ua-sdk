{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "../",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
    "typechain-types/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}